@model MVC.ViewModels.UserProfileViewModel

<div class="container">
    <div class="row">
        <div class="col-md-6">
            <h3>
                What should we write here?
            </h3>
        </div>
        <div class="col-md-6">
            <div class="row">
                <div class="col-md-6">
                    First name
                </div>
                <div class="col-md-6">
                    @Html.DisplayFor(m => m.FirstName)
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    Last name
                </div>
                <div class="col-md-6">
                    @Html.DisplayFor(m => m.LastName)
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    Email
                </div>
                <div class="col-md-6">
                    @Html.DisplayFor(m => m.Email)
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    User name
                </div>
                <div class="col-md-6">
                    @Html.DisplayFor(m => m.UserName)
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <a href="/User/EditProfile" class="btn btn-primary pull-right">Edit</a>
                </div>
            </div>
        </div>
    </div>
</div>
